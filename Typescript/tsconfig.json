{
  "compilerOptions": {
      "lib": ["es5", "es6", "dom","ESNext"],
      "target": "ES6",
      "rootDir": "./",
      "outFile": "../Runtime/Internal/JsRuntime.jspre",
      "strict": true,
      "noFallthroughCasesInSwitch":true,
      "importHelpers": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "strictNullChecks" : true,
      "alwaysStrict": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "baseUrl": ".",
  }
}
